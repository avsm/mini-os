prefix=${pcfiledir}/../..
exec_prefix=${prefix}
includedir=${prefix}/include/@OPAM_NAME@
libdir=${exec_prefix}/lib

Name: mini-os
Version: 0.10
URL: http://wiki.xen.org/wiki/Mini-OS
Description: A tiny OS kernel that can run under Xen
Cflags: -isystem${includedir} -isystem${includedir}/posix @ARCH_CFLAGS@ -nostdinc -isystem @GCC_INSTALL@include -U__linux__ -U__FreeBSD__ -U__sun__ -U__linux -D__MINIOS__ @PVFLAG@
Libs: -L${libdir}/@OPAM_NAME@ -lminios -T${libdir}/@OPAM_NAME@/libminios.lds @ARCH_LDFLAGS@
